{
  "name": "Crypto Pipeline Streaming",
  "dockerComposeFile": ["../docker-compose.yml"],
  "service": "stream",
  "workspaceFolder": "/usr/src/crypto-pipeline/streaming",
  "extensions": [
    "redhat.java",
    "vscjava.vscode-java-pack",
    "vscjava.vscode-java-debug",
    "vscjava.vscode-maven",
    "vscjava.vscode-java-dependency",
    "vscjava.vscode-java-test"
  ],
  "settings": {
    "java.home": "/usr/lib/jvm/java-17-openjdk"
  },
  "postCreateCommand": "java -version",
  "runServices": ["stream"],
  "shutdownAction": "none",
  "remoteUser": "root",
  "mounts": [
    "source=C:\\dev\\crypto-pipeline\\streaming\\.devcontainer,target=/.devcontainer,type=bind,consistency=cached"
  ]
}
